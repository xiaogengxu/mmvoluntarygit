@Article{Angerer2015,
  Title                    = {How to measure time preferences in children: a comparison of two methods},
  Author                   = {Angerer, Silvia and Lergetporer, Philipp and Gl{\"a}tzle-R{\"u}tzler, Daniela and Sutter, Matthias},
