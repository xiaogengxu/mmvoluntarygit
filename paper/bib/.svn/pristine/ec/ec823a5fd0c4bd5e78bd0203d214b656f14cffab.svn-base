  Keywords                 = {Adult; Analysis of Variance; Brain Mapping; Corpus Striatum, blood supply/physiology; Decision Making, physiology; Female; Humans; Image Processing, Computer-Assisted; Male; Oxygen, blood; Photic Stimulation, methods; Reaction Time; Reward; Social Perception; Task Performance and Analysis},
  Language                 = {eng},
  Medline-pst              = {ppublish},
