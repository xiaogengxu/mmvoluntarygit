@Article{Keil1986,
  Title                    = {Rules, reciprocity, and rewards: A developmental study of resource allocation in social interaction},
  Author                   = {Keil, Linda J.},
  Journal                  = {Journal of Experimental Social Psychology},
  Year                     = {1986},
  Pages                    = {419-435},
  Volume                   = {Volume 22},

  Abstract                 = {Most studies of children's fairness have been conducted in settings of minimal interdependence. However, understanding the development of fair behavior also requires exploration of the processes operating in settings of ongoing interaction. It was hypothesized that in such settings reciprocity would be an important determinant of children's reward allocations. Second and sixth grade boys and girls (N = 80) participated in a simulated interaction requiring them to take turns with a fictitious co-worker distributing earnings over a series of 10 trials. Co-worker's distributions were consistently selfish, generous, equitable, or equalitarian across trials. Results indicated, for both age groups, a strong influence of co-workers' prior distributions that increased across trials. For sixth graders, the modal distributions in each of the three experimental groups matched the co-workers'. They understood the rules of equity and equality and considered both fair. Neither selfishness nor generosity was considered fair, even as reciprocation of others' prior distribution. Second graders understood the equality norm although they tended to act selfishly and consider both equality and selfishness fair, despite having contributed less work than their co-workers. Generous and equitable co-workers influenced second graders' distributions away from selfishness but not enough to match the co-workers' distributions. Second graders considered neither equity nor generosity fair and displayed little understanding of the equity norm.},
  Owner                    = {s1964},
  Timestamp                = {2008.05.29}
}

