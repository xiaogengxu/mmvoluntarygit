@Misc{Bohren2017,
  Title                    = {The {D}ynamics of {D}iscrimination: {T}heory and {E}xperimental {E}vidence},

  Author                   = {Bohren, Aislinn J. and Imas, Alex and Rosenberg, Michael},
  HowPublished             = {Mimeo},
  Month                    = {May},
  Year                     = {2017},

  Abstract                 = {We study the dynamics of discrimination using a large-scale field experiment on an online Q&A forum. We randomly assign questions to post on accounts with male and female usernames and high and low reputation levels. Reputation is earned from votes for high quality posts and provides a signal of ability that is endogenously generated by other users on the site. We document significant discrimination, measured in terms of reputation points earned per question, against female accounts with no reputation compared to male accounts with no reputation. In contrast, the direction of discrimination reverses on high reputation accounts: questions posted to male accounts receive fewer reputation points than those posted to female accounts. We develop a theoretical framework and demonstrate how examining the dynamics of discrimination can help identify the mechanism behind discrimination when the true distributions of ability are unknown. We outline the dynamic predictions when discrimination is preference-based, belief-based with homogenous priors and belief-based with heterogeneous priors. Heterogeneity in beliefs implies that some individuals have misspecified models, such as in the case of stereotype bias. Our observed results are consistent with this latter case, and have significant implications for policies aimed at alleviating it.},
  Owner                    = {s11008},
  Timestamp                = {2017.05.03}
}

