@Article{Beaman2012,
  Title                    = {Female leadership raises aspirations and educational attainment for girls: a policy experiment in India},
  Author                   = {Lori Beaman and Esther Duflo and Rohini Pande and Petia Topalova},
  Journal                  = {Science},
