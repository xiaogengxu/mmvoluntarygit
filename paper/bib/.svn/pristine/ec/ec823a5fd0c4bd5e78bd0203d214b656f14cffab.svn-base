@Article{Platt1999,
  Title                    = {Neural correlates of decision variables in parietal cortex.},
  Author                   = {M. L. Platt and P. W. Glimcher},
  Journal                  = {Nature},
  Year                     = {1999},

  Month                    = {Jul},
  Number                   = {6741},
  Pages                    = {233--238},
  Volume                   = {400},

  Abstract                 = {Decision theory proposes that humans and animals decide what to do in a given situation by assessing the relative value of each possible response. This assessment can be computed, in part, from the probability that each action will result in a gain and the magnitude of the gain expected. Here we show that the gain (or reward) a monkey can expect to realize from an eye-movement response modulates the activity of neurons in the lateral intraparietal area, an area of primate cortex that is thought to transform visual signals into eye-movement commands. We also show that the activity of these neurons is sensitive to the probability that a particular response will result in a gain. When animals can choose freely between two alternative responses, the choices subjects make and neuronal activation in this area are both correlated with the relative amount of gain that the animal can expect from each response. Our data indicate that a decision-theoretic model may provide a powerful new framework for studying the neural processes that intervene between sensation and action.},
  Doi                      = {10.1038/22268},
  File                     = {Platt1999.pdf:mmbib/papers/Platt1999.pdf:PDF},
  Institution              = {Center for Neural Science, New York University, New York 10003, USA. mplatt@cns.nyu.edu},
  Keywords                 = {Animals; Color Perception, physiology; Decision Making, physiology; Eye Movements, physiology; Fixation, Ocular, physiology; Macaca; Models, Neurological; Motor Neurons, physiology; Neurons, Afferent, physiology; Neurons, physiology; Parietal Lobe, physiology; Probability; Reward},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {sameos},
  Pmid                     = {10421364},
