}

@Misc{Conte2009,
  Title                    = {``{T}he Pluralism of Fairness Ideals'': A Comment},

  Author                   = {Conte, Anna and Moffatt, Peter G.},
  HowPublished             = {Mimeo},
  Year                     = {2009},

  Abstract                 = {A recent paper by Cappelen et al. (2007) reports on the estimation of a finite mixture model of fairness using experimental data. Their estimation approach is based on a choice model. However, we argue strongly that since the data is not choice data, it should not be modelled as such. We adopt a different approach: modelling the decision variable directly, while incorporating the upper censoring, which is prominent in the data. We reach different conclusions from Cappelen et al. (2007): although we agree that "Strict Egalitarians" are the dominant type, we find that "Libertarians" barely exist. Our overall conclusion is that the practice of treating continuous data, or interval data, as if it were choice data is not innocuous, since it can lead to very different predictions. This conclusion is likely to be relevant to other contexts in which the decision variable is quantitative.},
  File                     = {Conte2009.pdf:mmbib\\papers\\Conte2009.pdf:PDF},
  Owner                    = {s1758},
  Timestamp                = {2009.04.04},
  Url                      = {http://ideas.repec.org/p/lui/wpaper/162.html}
}

