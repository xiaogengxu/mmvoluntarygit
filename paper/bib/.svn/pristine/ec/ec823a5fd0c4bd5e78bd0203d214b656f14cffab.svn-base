@Article{Brickman76,
  Title                    = {Equity versus equality as factors in children's moral judgments of thefts, charity, and third-party transfers},
  Author                   = {Brickman, Philip and Bryan, James H.},
  Journal                  = {Journal of Personality and Social Psychology},
  Number                   = {5},
  Pages                    = {757-761},
  Volume                   = {34},

  Abstract                 = {Previous research has found that charitable acts and 3rd-party transfers are more favorably judged when they increase equality than when they decrease equality. The present study determined whether this will be the case when the transfers decrease rather than increase the equity of the distribution and explored how equity and equality each affect Ss' judgments of fairness. 96 5th-grade boys and girls judged a model who made either a charitable, selfish, or 3rd-party transfer that either increased or decreased the equity of a distribution and either increased or decreased the equality of a distribution. Transfers were favorably judged when they (a) increased equity, regardless of whether they increased or decreased equality; and (b) when they increased equality, regardless of whether they increased or decreased equity. Data indicate that equity and equality preferences can operate independently and simultaneously and provide a general method by which their relative strength can be assessed.},
  Doi                      = {10.1037/0022-3514.34.5.757},
