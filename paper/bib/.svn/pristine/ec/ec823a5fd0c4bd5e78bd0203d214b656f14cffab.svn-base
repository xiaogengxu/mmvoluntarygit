@Article{Kern2006,
  Title                    = {Elegance, silence and nonsense in the mutations literature for solid tumors.},
  Author                   = {Scott E Kern and Jordan M Winter},
  Journal                  = {Cancer Biol Ther},
  Year                     = {2006},

  Month                    = {Apr},
  Number                   = {4},
