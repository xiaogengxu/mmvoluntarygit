@Misc{Konow2009,
  Title                    = {Morals and Mores? {E}xperimental Evidence on Equity and Equality},

  Author                   = {Konow, James and Saijo, Tatsuyoshi and Akai, Kenju},
  HowPublished             = {Mimeo, Loyola Marymount University},
  Year                     = {2009},

  Abstract                 = {The growing recognition of fairness as an important economic force has not been accompanied by a comparable convergence of opinion as to the appropriate rule of justice. Nevertheless, among the possible concepts, the primary rivals in most of the philosophical and social science literature are equality, or equal outcomes, and equity, by which we mean outcomes that are proportional to individual contributions. This paper proposes and tests experimentally two classes of moral preferences, morals and mores, which, in the context of distributive justice, correspond respectively to equity and equality. Morals are the moral preferences of impartial third parties, or spectators, whereas mores are the moral preferences that apply in personal relationships involving stakeholders. We report the results of a dictator experiment that confirm this distinction: spectators prefer equity and stakeholders trade off equity and equality, whereby this tradeoff responds, as predicted, to the degree of anonymity. Potential effects on dictator allocations of a large number of variables are also tested, including gender, race, income, culture, and nationality (American or Japanese), and we find that these non-ethical variables rarely matter. We argue that the proposed distinction reconciles ostensibly conflicting results on equity and equality from the laboratory and the field and that the experiment provides interesting results for behavioral theories, normative analysis and economic policy.},
  File                     = {Konow2009.pdf:mmbib/papers/Konow2009.pdf:PDF},
  Owner                    = {s1579},
  Timestamp                = {2008.12.30},
  Url                      = {http://myweb.lmu.edu/jkonow/Morals%20and%20Mores.pdf}
}

