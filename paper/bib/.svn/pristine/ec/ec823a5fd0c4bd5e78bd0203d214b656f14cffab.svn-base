@Article{Booth2012,
  Title                    = {Choosing to compete: How different are girls and boys?},
  Author                   = {Booth, Alison and Nolen, Patrick},
  Journal                  = {Journal of Economic Behavior \& Organization},
  Year                     = {2012},
  Number                   = {2},
  Pages                    = {542-555},
  Volume                   = {81},

  Abstract                 = {Using a controlled experiment, we examine the role of nurture in explaining the stylized fact that women shy away from competition. We have two distinct research questions. First, does the gender composition of the group to which a student is randomly assigned affect competitive choices? Second, does the gender mix of the school a student attends affect competitive choices? Our subjects (students just under 15 years of age) attend publicly funded single-sex and coeducational schools. We find robust differences between the competitive choices of girls from single-sex and coed schools. Moreover, girls from single-sex schools behave more like boys even when randomly assigned to mixed-sex experimental groups. This suggests that it is untrue that the average female avoids competitive behavior more than the average male. It also suggests that observed gender differences might reflect social learning rather than inherent gender traits.},
  Keywords                 = {Tournament; Piece-rate; Gender; Experiment; Competitive behavior},
  Url                      = {http://ideas.repec.org/a/eee/jeborg/v81y2012i2p542-555.html}
}

